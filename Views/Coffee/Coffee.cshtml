                            </div>
                            <div class="col-lg-3 col-md-6 p-4">
                                <div class="coffee_img"><img src="~/Content/images/img-3.png"></div>
                                <h3 class="types_text">CÀ PHÊ SỮA</h3>
                                <p class="price-pro alata-regular">50,000 VNĐ</p>
                            </div>
                            <div class="col-lg-3 col-md-6 p-4">
                                <div class="coffee_img"><img src="~/Content/images/img-4.png"></div>
                                <h3 class="types_text">PHINDI HẠNH NHÂN</h3>
                                <p class="price-pro alata-regular">45,000 VNĐ</p>
                            </div>
                            <div class="col-lg-3 col-md-6 p-4">
                                <div class="coffee_img"><img src="~/Content/images/img-3.png"></div>
                                <h3 class="types_text">BẠC XỈU</h3>
                                <p class="price-pro alata-regular">35,000 VNĐ</p>
                            </div>
                            <div class="col-lg-3 col-md-6 p-4">
                                <div class="coffee_img"><img src="~/Content/images/img-4.png"></div>
                                <h3 class="types_text">CAPPUCCINO</h3>
                                <p class="price-pro alata-regular">69,000 VNĐ</p>
                            </div>
                            <div class="col-lg-3 col-md-6 p-4">
                                <div class="coffee_img"><img src="~/Content/images/img-3.png"></div>
                                <h3 class="types_text">PHINDI CASSIA</h3>
                                <p class="price-pro alata-regular">55,000 VNĐ</p>
                            </div>
                            <div class="col-lg-3 col-md-6 p-4">
                                <div class="coffee_img"><img src="~/Content/images/img-4.png"></div>
                                <h3 class="types_text">PHINDI KEM SỮA</h3>
                                <p class="price-pro alata-regular">60,000 VNĐ</p>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            @*<a class="carousel-control-prev" href="#main_slider" role="button" data-slide="prev">
                    <i class="fa fa-arrow-left"></i>
                </a>
                <a class="carousel-control-next" href="#main_slider" role="button" data-slide="next">
                    <i class="fa fa-arrow-right"></i>
                </a>*@
        </div>
    </div>
</div>

